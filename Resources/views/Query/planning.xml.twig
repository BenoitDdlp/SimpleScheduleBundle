<?xml version="1.0" encoding="UTF-8"?>
<planning>
    {% for key, dayTimeTasks in allTasks %}
    <{{ key }}> 
        {% for key, dayTasks in dayTimeTasks %}
            <{{ key }}>
            <tasks>
            {% for task in dayTasks %}
                
                    {% include 'IDCISimpleScheduleBundle:Query:task2.xml.twig' with {'task': task} %}
            
            {% endfor %}
            </tasks>
            </{{ key }}>
        {% endfor %}
    </{{ key }}>
    {% endfor %}
</planning>